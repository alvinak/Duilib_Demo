							<Control width="5"/>
							<Edit name="edit_patient_csv_path" inset="5,0,5,0" wordbreak="true" tipvalue="pid列表文件路径..." textpadding="5,0,0,0" style="edit_style_dicom"/>
						</HorizontalLayout>
						<Control height="1"/>
						<HorizontalLayout bkcolor="#FF11172D" height="25"> 
