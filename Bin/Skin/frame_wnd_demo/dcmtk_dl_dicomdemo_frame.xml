						<HorizontalLayout bkcolor="#FF272B2E" height="30"> 
							<Button name="btn_filter" text="搜索" width="80" tooltip="搜索" textcolor="#FFF2F2F2" hottextcolor="#FF333333" normalimage="file='sysbtn\button_normal.png' corner='1,1,1,1'" hotimage="file='sysbtn\button_hover.png' corner='1,1,1,1'" pushedimage="file='sysbtn\button_pushed.png' corner='1,1,1,1'"/>
							<Control width="5"/>
							<Button name="btn_download" text="下载" width="80" tooltip="下载" textcolor="#FFF2F2F2" hottextcolor="#FF333333" normalimage="file='sysbtn\button_normal.png' corner='1,1,1,1'" hotimage="file='sysbtn\button_hover.png' corner='1,1,1,1'" pushedimage="file='sysbtn\button_pushed.png' corner='1,1,1,1'"/>
							<Control width="5"/>
							<Button name="btn_download_path" text="选择路径" width="80" tooltip="下载建立" textcolor="#FFF2F2F2" hottextcolor="#FF333333" normalimage="file='sysbtn\button_normal.png' corner='1,1,1,1'" hotimage="file='sysbtn\button_hover.png' corner='1,1,1,1'" pushedimage="file='sysbtn\button_pushed.png' corner='1,1,1,1'"/>
							<Control width="5"/>
							<Edit name="edit_download_path" inset="5,0,5,0" wordbreak="true" tipvalue="下载路径..." textpadding="5,0,0,0" />
