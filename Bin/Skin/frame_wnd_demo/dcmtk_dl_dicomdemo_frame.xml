							<Control width="2"/>
							<Button name="btn_patient_csv_path" text="选择文件" width="80" tooltip="选择csv格式的Patient ID列表文件" textcolor="#FFF2F2F2" hottextcolor="#FF333333" normalimage="file='sysbtn\button_normal.png' corner='1,1,1,1'" hotimage="file='sysbtn\button_hover.png' corner='1,1,1,1'" pushedimage="file='sysbtn\button_pushed.png' corner='1,1,1,1'"/>
