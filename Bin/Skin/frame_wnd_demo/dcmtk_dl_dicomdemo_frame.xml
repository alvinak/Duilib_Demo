							<Control width="20"/>
							<Edit name="edit_filter_thickness" inset="5,0,5,0" tipvalue="mm" textpadding="5,0,0,0" style="edit_style_dicom"/>				
						</HorizontalLayout>
						<HorizontalLayout height="25" bkcolor="#FF11172D"> 
