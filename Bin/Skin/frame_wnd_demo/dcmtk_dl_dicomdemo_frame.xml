						<HorizontalLayout height="25" bkcolor="#FF272B2E"> 
							<Option name="op_device_modality" text="设备     (0008|0061)" width="160" userdata="RF" style="pacs_cb_style"/>
							<Control width="20"/>
							<Edit name="edit_filter_modality" inset="5,0,5,0" tipvalue="modality" textpadding="5,0,0,0" />				
